{
  "name": "electro-git",
  "version": "1.0.0",
  "description": "A GIT GUI built with electron.JS and React.js",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/burwinliu/electro-git"
  },
  "bugs": {
    "url": "https://github.com/burwinliu/electro-git/issues"
  },
  "author": {
    "name": "Burwin Liu",
    "email": "burwinliu1@gmail.com",
    "url": "https://github.com/burwinliu"
  },
  "keywords": [
    "app",
    "boilerplate",
    "electron",
    "git",
    "open-source",
    "react",
    "reactjs",
    "source",
    "webpack"
  ],
  "engines": {
    "node": ">=9.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "browserslist": [
    "defaults"
  ],
  "main": "app/main.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "build": "cross-env NODE_ENV=production webpack --config webpack.build.config.js --mode production",
    "package": "yarn build",
    "postpackage": "rimraf builds && electron-packager ./ --out=./builds"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "chokidar": "^3.4.2",
    "core-js": "^3.6.5",
    "node-rsa": "^1.1.1",
    "nodegit": "^0.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "simple-git": "^2.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "babel-minify": "^0.5.1",
    "cross-env": "^7.0.2",
    "css-loader": "^4.0.0",
    "electron": "^9.4.0",
    "electron-devtools-installer": "^3.0.0",
    "electron-packager": "^15.0.0",
    "electron-rebuild": "^2.0.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-pxtorem": "^5.1.1",
    "style-loader": "^1.2.0",
    "terser-webpack-plugin": "^3.0.8",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
